{
  "name": "shot-expense",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 && tsc --noEmit",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "cap:sync": "pnpm build && cap sync",
    "cap:add:android": "cap add android",
    "cap:android": "pnpm cap:sync && cd android && ./gradlew assembleDebug",
    "cap:android:release": "pnpm cap:sync && cd android && ./gradlew assembleRelease",
    "cap:android:bundle": "pnpm cap:sync && cd android && ./gradlew bundleRelease"
  },
  "dependencies": {
    "@capacitor/core": "^5.7.0",
    "@capacitor/preferences": "^5.0.7",
    "date-fns": "^3.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@capacitor/android": "^5.7.0",
    "@capacitor/cli": "^5.7.0",
    "@eslint/js": "^9.39.2",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.54.0",
    "vite": "^5.0.8"
  }
}
